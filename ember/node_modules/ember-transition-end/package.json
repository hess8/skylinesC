{
  "name": "ember-transition-end",
  "version": "1.0.0",
  "description": "Cross-browser transtionend event listener",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BBVAEngineering/cross-transition-end.git"
  },
  "license": "MIT",
  "keywords": [
    "ember-addon",
    "browser",
    "cross-browser",
    "cross",
    "transition",
    "css",
    "animations",
    "listener",
    "callback",
    "event",
    "end"
  ],
  "author": "BBVAEngineering",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --max-warnings 0 ./*.js addon app config tests",
    "start": "ember serve",
    "test": "COVERAGE=true ember test",
    "check-coverage": "istanbul check-coverage coverage/coverage-final.json --statements 100 --functions 100 --branches 100 --lines 100",
    "commit": "git-cz",
    "posttest": "npm run report-coverage && npm run check-coverage && npm run validate-coverage",
    "report-coverage": "istanbul report --include=coverage/coverage-final.json text",
    "semantic-release": "semantic-release",
    "validate-coverage": "test \"$(cat coverage/coverage-summary.json | json total.lines.total)\" -gt 0"
  },
  "dependencies": {
    "ember-cli-babel": "^7.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@ember/optional-features": "^1.1.0",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-test-helper": "^2.0.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "ember-auto-import": "^1.5.2",
    "ember-cli": "~3.14.0",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-htmlbars": "^4.0.8",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-engines": "^0.8.2",
    "ember-export-application-global": "^2.0.0",
    "ember-intl": "^4.2.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "^3.14.1",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.2.1",
    "eslint": "^6.6.0",
    "eslint-config-bbva": "^2.0.4",
    "eslint-plugin-ember": "^7.2.0",
    "eslint-plugin-node": "^10.0.0",
    "husky": "^3.1.0",
    "istanbul": "^0.4.5",
    "json": "^9.0.6",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.9.0",
    "semantic-release": "^15.13.31",
    "sinon": "^7.5.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
